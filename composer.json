{
    "name": "contao-community-alliance/vcs-synchronizer",
    "description": "Keep multiple VCS repositories in sync.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "contao-community-alliance/build-system-repository-git": "~1.0",
        "monolog/monolog": "~1.0",
        "symfony/console": "~2.3",
        "symfony/event-dispatcher": "~2.3",
        "symfony/monolog-bridge": "~2.3"
    },
    "require-dev": {
        "contao-community-alliance/build-system": "~1.0",
        "contao-community-alliance/coding-standard": "~1.0",
        "phpunit/phpunit": "3.7.*",
        "phpmd/phpmd": "~2.0",
        "sebastian/phpcpd": "~2.0",
        "squizlabs/php_codesniffer": "~1.0",
        "phploc/phploc": "~2.0",
        "symfony/filesystem": "~2.5",
        "symfony/process": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "ContaoCommunityAlliance\\BuildSystem\\VcsSync\\": "src",
            "ContaoCommunityAlliance\\BuildSystem\\VcsSync\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
